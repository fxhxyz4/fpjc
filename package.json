{
  "name": "fpcoffee",
  "version": "1.0.0",
  "description": "Functional utilities library for coffee/js",
  "keywords": [
    "functional programming",
    "coffeescript",
    "javascript",
    "library",
    "fp"
  ],
  "author": {
    "name": "fxhxyz4",
    "email": "fxhxyz@proton.me",
    "url": "https://fxhxyz.vercel.app/"
  },
  "homepage": "https://github.com/fxhxyz4/fpcoffee",
  "bugs": "https://github.com/fxhxyz4/fpcoffee/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/fpcoffee.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhxyz@proton.me"
    }
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "lint-config": "coffeelint --makeconfig > coffeelint.json",
    "lint": "coffeelint -f coffeelint.json ./src/*.coffee ./examples/*.coffee ./dist/*.coffee",
    "prettier": "prettier ./src/*.coffee ./examples/*.coffee ./dist/*.coffee --write",
    "babel": "babel dist/*.js -d dist/ --presets=@babel/preset-env",
    "build": "rollup -c",
    "w": "webpack -c ./webpack.config.js --mode=production",
    "test": "coffee ./examples/add.coffee"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@coffeelint/cli": "^5.2.11",
    "coffeescript": "2.7.0",
    "prettier-plugin-coffeescript": "^0.1.5",
    "rollup": "4.1.1",
    "rollup-plugin-coffee-script": "2.0.0"
  }
}
